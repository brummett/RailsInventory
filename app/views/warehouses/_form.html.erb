<%= form_for @warehouse do |f| %>
    <% if @warehouse.errors.any? %>
        <div id="error_explanation">
            <h2>
                <%= pluralize(@warehouse.errors.count, 'error') %> prohibited this warehouse from saving
            </h2>
            <ul>
                <% @warehouse.errors.full_messages.each do |msg| %>
                    <li><%= msg %></li>
                <% end %>
            </ul>
        </div>
    <% end %>

    <p>
        <%= f.label :name, 'Name' %> <%= f.text_field :name %>
        <br>
        <%= f.label :email, 'Email' %> <%= f.text_field :email %>
        <br>
        <%= f.label :phonenumber, 'Phone' %> <%= f.text_field :phonenumber %>
        <br>
        <%= f.label :address, 'Address' %> <%= f.text_area :address %>
        <br>
        <%= f.label :notes, 'Notes' %> <%= f.text_area :notes %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
